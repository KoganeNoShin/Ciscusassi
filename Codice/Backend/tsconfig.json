{
	"compilerOptions": {
		"module": "CommonJS",
		"target": "ES2020",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"strict": true,
		"rootDir": "./",
		"outDir": "./dist"
	},
	"include": ["./**/*.ts", "Database/Seeders/seederAsporto.js"], // include tutti i file TS nel progetto
	"exclude": ["node_modules", "dist"]
}
